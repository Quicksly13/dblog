{% extends "page.html.twig" %} 


{% block navbar %} 



{% endblock navbar %}

{% block content %}

<div class="container-fluid w-50" style="margin-top:2em;margin-bottom:2em">

  {% if form is defined %}

  
  {{ form_start(form) }} 
    
  {{ form_widget(form) }}

  {{ form_end(form) }}

    
  {% else %}

  {{ message }}
    
  {% endif %}




</div>

<nav aria-label="Page navigation example">
  <ul class="pagination justify-content-center">
    <li class="page-item">
      <a class="page-link" href="#" aria-label="Previous">
        <span aria-hidden="true">&laquo;Read the previous</span>
        <span class="sr-only">Previous</span>
      </a>
    </li>


    <li class="page-item">
      <a class="page-link" href="#" aria-label="Next">
        <span aria-hidden="true">Read the next&raquo;</span>
        <span class="sr-only">Next</span>
      </a>
    </li>
  </ul>
</nav>

{% endblock %}